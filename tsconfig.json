{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true,
        "jsx": "react",
        "lib": [
            "es6",
            "dom"
        ],
        "removeComments": true,
        "typeRoots": [
            "/node_modules/@types",
            "/Types/"
        ]
    },
    "compileOnSave": false,
    "exclude": [
        "/node_modules/",
        "/bin",
        "/obj"
    ]
}